@model IEnumerable<Software2.Models.Formula>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Mascota.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Practicante.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Mascota.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Practicante.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.formulaID }) |
            @Html.ActionLink("Details", "Details", new { id=item.formulaID }) |
